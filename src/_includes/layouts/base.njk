<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
    <link rel="icon" type="image/png" href="/icon-48x48.png">
    {% block head -%}
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/katex@0.11.1/dist/katex.min.css">
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/prismjs@1.20.0/themes/prism.css">
    <link rel="stylesheet" href="{{ '/css/styles.css' | url }}">
    {%- endblock %}
    {# <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}"> #}
  </head>
  <body>
    {% block content -%}
    {{ content | safe }}
    {%- endblock %}
    {% block bodyend -%}
    {%- if environment.eleventy_env == "production" -%}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{ environment.ga_tracking_id }}"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', '{{ environment.ga_tracking_id }}');
    </script>
    {%- endif -%}
    {%- endblock %}
  </body>
</html>