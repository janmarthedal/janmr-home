{
  "name": "gatsby-remark-mathjax",
  "description": "Transform math nodes to html markup",
  "version": "0.9.0",
  "dependencies": {
    "@babel/runtime": "^7.8.7",
    "remark-math": "^1.0.6",
    "unist-util-visit": "^1.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "babel-preset-gatsby-package": "^0.3.1",
    "cross-env": "^5.2.1"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "mathjax",
    "remark"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "mathjax-node": "^2.1.0"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "engines": {
    "node": ">=10.13.0"
  }
